{
  "name": "place",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000 -H $(ip addr show eth0 | grep 'inet\\b' | awk '{print $2}' | cut -d/ -f1)",
    "build": "next build",
    "start": "next start -p 4000 -H $(ip addr show eth0 | grep 'inet\\b' | awk '{print $2}' | cut -d/ -f1)",
    "lint": "next lint",
    "prod": "yarn build && yarn start",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "next": "^12.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.49.0"
  },
  "devDependencies": {
    "@arthurfiorette/prettier-config": "^1.0.7",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "typescript": "^4.5.5"
  }
}
