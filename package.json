{
  "name": "place",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000 -H $(ip addr show eth0 | grep 'inet\\b' | awk '{print $2}' | cut -d/ -f1)",
    "start": "next start -p 4000 -H $(ip addr show eth0 | grep 'inet\\b' | awk '{print $2}' | cut -d/ -f1)",
    "build": "next build",
    "postbuild": "next-sitemap",
    "lint": "next lint",
    "prod": "yarn build && yarn start",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.5.0",
    "marked": "^4.0.12",
    "next": "^12.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "sass": "^1.49.9",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@arthurfiorette/prettier-config": "^1.0.7",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-config-next": "^12.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "next-sitemap": "^2.5.14",
    "prettier": "^2.6.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "typescript": "^4.6.3"
  }
}
