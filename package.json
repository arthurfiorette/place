{
  "name": "place",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000 -H $(ip addr show eth0 | grep 'inet\\b' | awk '{print $2}' | cut -d/ -f1)",
    "start": "next start -p 4000 -H $(ip addr show eth0 | grep 'inet\\b' | awk '{print $2}' | cut -d/ -f1)",
    "build": "next build",
    "postbuild": "next-sitemap",
    "lint": "next lint",
    "prod": "yarn build && yarn start",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@octokit/rest": "^19.0.3",
    "disqus-react": "^1.1.3",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.6.0",
    "marked": "^4.0.17",
    "next": "^12.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "sass": "^1.53.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@arthurfiorette/prettier-config": "^1.0.7",
    "@types/marked": "^4.0.3",
    "@types/node": "^18.7.12",
    "@types/react": "^18.0.17",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "next-sitemap": "^3.1.17",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.0",
    "typescript": "^4.7.4"
  }
}
