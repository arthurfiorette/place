{
  "name": "place",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000 -H $(ip addr show eth0 | grep 'inet\\b' | awk '{print $2}' | cut -d/ -f1)",
    "start": "next start -p 4000 -H $(ip addr show eth0 | grep 'inet\\b' | awk '{print $2}' | cut -d/ -f1)",
    "build": "next build",
    "postbuild": "next-sitemap",
    "lint": "next lint",
    "prod": "yarn build && yarn start",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@octokit/rest": "^19.0.7",
    "disqus-react": "^1.1.5",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.7.0",
    "marked": "^4.3.0",
    "next": "^13.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "sass": "^1.62.0",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@arthurfiorette/prettier-config": "^1.0.7",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "next-sitemap": "^3.1.55",
    "prettier": "^2.8.7",
    "prettier-plugin-organize-imports": "^3.2.2",
    "typescript": "^4.8.4"
  }
}
